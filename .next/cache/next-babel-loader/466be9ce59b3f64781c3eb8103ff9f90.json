les} from './useStyle'\nimport {ButtonComponent} from '../../elements/button/Button'\n\n\nexport const Header = () => {\n    const [adType, setAdType] = useState('');\n\n    const handleChange = (e) => {\n        setAdType(e.target.value);\n    };\n\n    const classes = useStyles();\n    return (\n        <AppBar\n            className={classes.root}\n            position=\"fixed\"\n            color={'default'}\n            elevation={0}\n        >\n            {/* Header bottom start */}\n            <Container maxWidth=\"lg\">\n                <Grid container justify=\"space-between\" alignItems=\"center\">\n                    <Grid container item xs={3} spacing={1} alignItems=\"center\">\n                        <Grid container item xs={6} className=\"bottom-logo\">\n                            <a href=\"#\">\n                                <img src={Logo} alt=\"Slondo logo\"/>\n                            </a>\n                        </Grid>\n                        <Grid item container xs={6} justify=\"center\">\n                            <Button\n                                variant=\"contained\"\n                                color=\"primary\"\n                                className=\"category-button\"\n                            >\n                                <Typography variant=\"h6\">Категории</Typography>\n                                <img\n                                    src={CategoryIcon}\n                                    style={{\n                                        marginLeft: '5px',\n                                        height: '15px',\n                                    }}\n                                    alt=\"Category icon\"\n                                />\n                            </Button>\n                        </Grid>\n                    </Grid>\n                    <Grid\n                        item\n                        container\n                        md={8}\n                        xs={12}\n                        spacing={1}\n                        alignItems=\"center\"\n                        className=\"search-block\"\n                    >\n                        <Grid item md={6} sm={12} xs={12}>\n                            <div>\n                                <form>\n                                    <img\n                                        src={SearchIcon}\n                                        className=\"search-icon\"\n                                    />\n                                    <input\n                                        type=\"text\"\n                                        className=\"search-input\"\n                                        placeholder=\"Поиск по объявлениям\"\n                                    />\n                                    <Button className=\"search-button\">\n                                        <Typography variant=\"h6\">\n                                            Найти\n                                        </Typography>\n                                    </Button>\n                                </form>\n                            </div>\n                        </Grid>\n                        <Grid className='select-menu' item md={3}>\n                            <Select\n                                variant={'outlined'}\n                                value={adType}\n                                onChange={handleChange}\n                                displayEmpty\n                            >\n                                <MenuItem value=\"\">\n                                    <Typography>Тип объявления</Typography>\n                                </MenuItem>\n                                <MenuItem value={10}>\n                                    <Typography>Ten</Typography>\n                                </MenuItem>\n                                <MenuItem value={20}>\n                                    <Typography>Twenty</Typography>\n                                </MenuItem>\n                                <MenuItem value={30}>\n                                    <Typography>Thirty</Typography>\n                                </MenuItem>\n                            </Select>\n                        </Grid>\n                        <Grid item md={3}>\n                            <Button\n                                variant=\"contained\"\n                                color=\"primary\"\n                                fullWidth\n                                className=\"add-adv\"\n                            >\n                                <Typography variant=\"h6\">\n                                    Создать объявление\n                                </Typography>\n                                <img\n                                    src={AddIcon}\n                                    style={{\n                                        marginLeft: '10px',\n                                        height: '20px',\n                                    }}\n                                />\n                            </Button>\n                        </Grid>\n                    </Grid>\n                    <Grid item container alignItems=\"center\" xs={1}>\n                        <ButtonComponent\n                            variant=\"contained\"\n                            fullWidth\n                            className=\"bottom-sign-button\"\n                        >\n                            <Typography variant=\"h6\">Вход</Typography>\n                            <img\n                                src={SignIcon}\n                                style={{\n                                    width: '20px',\n                                    marginLeft: '10px',\n                                }}\n                                alt=\"Sign in\"\n                            />\n                        </ButtonComponent>\n                    </Grid>\n      